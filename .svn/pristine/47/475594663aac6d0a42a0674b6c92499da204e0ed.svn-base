@charset "utf-8";
/* CSS Document */
/* index */
html {
  width: 100%;
  height: 100%;
  overflow: hidden;
}
body {
  width: 100%;
  height: 100%;
  text-align: center;
  font-family: "Arial","Microsoft YaHei",tahoma,arial,'Hiragino Sans GB',sans-serif;
}
/* CSS Tools */
.tool-over-hide { overflow: hidden; }
/* grid */

/* module */
.m-header { }
.m-footer { }
