.m-loginBox { margin: 0 30px; padding: 0; border: 1px solid #5B5B5B ;background: rgba(0, 0, 0, 0.41);height: 68%;}
.m-formBox { padding: 0 0 30px; border-top: transparent 1px solid;}


.m-formBox_ins { width: 320px; margin: 0 auto; min-height: 200px; text-align: center; }
.m-radioBox { height: 40px; text-align: left; color: #fff;}
.m-radioBox .u-radio { margin-right: 8px;}

.m-tabBox { position: relative;}
.m-tabBox .list { min-height:65px;}
.m-tabBox .list li {}
.m-tabBox .list li .text_a { display: block; height:65px; line-height: 65px; text-align: center; 
  border: transparent 1px solid; 
  color: #fff; font-size: 16px; }
.m-tabBox .list li:hover .text_a { color: #f0f0f0;}
.m-tabBox .list li.mark .text_a { border-radius:0;background: url(../img/bg_color_a20_0.png) 0 0 repeat; }
.m-tabBox .list_float li { float: left;}
.m-tabBox .list_2X li { width: 49.9%;}

.m-login_logoImg { display:inline-block; width: auto; height:82px;}
.m-login_logoText { display:block; margin-bottom: 20px; height: 60px; line-height: 60px;}
.m-login_logoText .text_span { text-shadow: 1px 1px 3px #000b1d; font-size: 20px; color: #fff;  }

