	<body style="height: 100%;width:100%;">		
	<div class="main-container" id="main-container">
			<div class="main-container-inner">
				<div class="main-content" style="margin-left:0px;">
					<div class="breadcrumbs" id="breadcrumbs" style="height:45px; background-color: #83BFEA;">
						<ul class="breadcrumb" style="margin-top:10px; margin-left:20px;">
							<li>
								<i class="icon-edit" style="color:#F4F8FF;"></i>
								<i style="color:white; font-size:18px; font-family:KaiTi;">系统管理</i>
							</li>
							<li>
								<i style="color:white; font-size:18px; font-family:KaiTi;">指挥机管理</i>
							</li>
						</ul>
					</div>
			<!--新增指挥机  -->			
          <div id="saveZhj" title="指挥机操作" style="display:none;"> 
               <form id="saveZhjForm" enctype="multipart/form-data" onsubmit="return checkInfo();">
    	           <table class="exhibit_table" style="font-size:13px; text-align:left;">  										    
				      <tr>  								        
				         <td>
				        	<input type="hidden" id="uuid" name="id" style="width:230px;"/>
				         </td>
				      </tr>				  
				     <tr>  
				        <td style="width:90px;" align="right">用户名称: <font color="red">*</font></td>
				        <td style="padding:10px;" class="input-icon input-icon-right">				        	
						 <input class ="inputName" type="text" name="name" id="zhjNameIn" autocomplete="off" maxlength="20" style="width: 230px;"/>  						   						  
				        </td>
				    </tr>	
				    <tr>  
				        <td style="width:90px;" align="right">指挥机类型: <font color="red">*</font></td>
				        <td style="padding:10px;" class="input-icon input-icon-right">				        	
							<select id= "zhjType" name = "zhjType">
								<option value="0">普通指挥机</option>
								<option value="1">中心指挥机</option>
							</select> 						   						  
				        </td>
				    </tr>
				    
				    <tr>  
				        <td style="width:90px;" align="right">所属区域: <font color="red">*</font></td>
				    </tr>
				    <tr>
				    	<td style = "width:90px;" align="right">省 ：</td>
				    	<td style="padding:10px;" class="input-icon input-icon-right">
				    		<select  style=" width: 230px;" id = "province" name ="province"  onchange ="getCity()">
						 
						 </select>
				    	</td>
				    </tr>
				    
				    <tr>
				    	<td style = "width:90px;" align="right">市 ：</td>
				    	<td style="padding:10px;" class="input-icon input-icon-right">
				    		<select  style=" width: 230px;" id = "city" name ="city"   onchange ="getCounty()">
						 
						 </select>
				    	</td>
				    </tr>
				    
				    <tr>
				    	<td style = "width:90px;" align="right">区/县：</td>
				    	<td style="padding:10px;" class="input-icon input-icon-right">
				    		<select  style=" width: 230px;" id = "county" name ="county"  >
						 
						 </select>
				    	</td>
				    </tr>
				    
				    <tr>  
				        <td style="width:90px;" align="right">卡号: <font color="red">*</font></td>
				        <td style="padding:10px;" class="input-icon input-icon-right">				        	
						 <input type="text" name="cardNum" id="cardNum" autocomplete="off" maxlength="50" style="width: 230px;"/>  						   						  
				        </td>
				    </tr>	
				   
				    <tr>  
				        <td style="width:90px;" align="right">所属行业: <font color="red">*</font></td>
				        <td style="padding:10px;" class="input-icon input-icon-right">				        	
						 <select  style=" width: 230px;" id = "industName" name ="industName"  >
						 
						 </select> 						   						  
				        </td>
				    </tr>
				   
				    <tr>  
				        <td style="width:90px;" align="right">备注:</td>
				        <td style="padding:10px;" class="input-icon input-icon-right">				        	
						 <input type="text" name="remark" id="remark" autocomplete="off" maxlength="50" style="width: 230px;"/>  						   						  
				        </td>
				    </tr>							
				   										   
					<tr>
					    <td colspan='2'>
					    	<button class="btn btn-info btn-sm" type="submit" onclick="editZhj()" style='border-radius:8px; margin-left:27%;'>
								<i class="icon-ok bigger-110"></i>确定
							</button>
						    <span class="btn btn-sm" onclick="$('#saveZhj').dialog('close')" style='border-radius:8px; margin-left:40px;'>
								<i class="icon-remove bigger-110"></i>取消
							</span>
						</td>
					</tr>  
				</table>					   					   				
	 		</form>								
		</div>
		<!-- 修改指挥机 -->	
		<div id="updateZhj" title="指挥机操作" style="display:none;"> 
               <form id="updateZhjForm" enctype="multipart/form-data" onsubmit="return checkRule();">
    	           <table class="exhibit_table" style="font-size:13px; text-align:left;">  										    
				      <tr>  								        
				         <td>
				        	<input type="hidden" id="uuid1" name="id" style="width:230px;"/>
				         </td>
				      </tr>				  
				     <tr>  
				        <td style="width:90px;" align="right">指挥机名称: <font color="red">*</font></td>
				        <td style="padding:10px;" class="input-icon input-icon-right">				        	
						 <input type="text" name="name" id="updateZhjName" autocomplete="off" maxlength="50" style="width: 230px;"/>  						   						  
				        </td>
				    </tr>	
				    <tr>  
				        <td style="width:90px;" align="right">指挥机类型: <font color="red">*</font></td>
				        <td style="padding:10px;" class="input-icon input-icon-right">				        	
						<!--  <input type="text" name="type" id="type" autocomplete="off" maxlength="50" style="width: 230px;"/> --> 
							<select id= "updateZhjType" name = "zhjType">
								<option value="0">普通指挥机</option>
								<option value="1">中心指挥机</option>
							</select> 						   						  
				        </td>
				    </tr>
				   
				    <tr>  
				        <td style="width:90px;" align="right">所属区域: <font color="red">*</font></td>
				    </tr>
				    <tr>
				    	<td style = "width:90px;" align="right">省 ：</td>
				    	<td style="padding:10px;" class="input-icon input-icon-right">
				    		 <select  style=" width: 230px;" id = "updateProvince" name ="province" onchange ="getUpdateCity()" ></select>
				    	</td>
				    </tr>
				    
				    <tr>
				    	<td style = "width:90px;" align="right">市 ：</td>
				    	<td style="padding:10px;" class="input-icon input-icon-right">
				    		<select  style=" width: 230px;" id = "updateCity" name ="city" onchange ="getUpdateCounty()" ></select>
				    	</td>
				    </tr>
				    
				    <tr>
				    	<td style = "width:90px;" align="right">区/县：</td>
				    	<td style="padding:10px;" class="input-icon input-icon-right">
							<select  style=" width: 230px;" id = updateCounty name ="county"  ></select>
				    	</td>
				    </tr>
				    
				     <tr>  
				        <td style="width:90px;" align="right">卡号: <font color="red">*</font></td>
				        <td style="padding:10px;" class="input-icon input-icon-right">				        	
						 <input type="text" name="cardNum" id="updateCardNum" autocomplete="off" maxlength="50" style="width: 230px;"/>  						   						  
				        </td>
				    </tr>	
				    
				     <tr>  
				        <td style="width:90px;" align="right">所属行业: <font color="red">*</font></td>
				        <td style="padding:10px;" class="input-icon input-icon-right">				        	
						 <select  style=" width: 230px;" id = "updateIndustName" name ="industName"  >
						 
						 </select> 						   						  
				        </td>
				    </tr>
				    
				    
				    <tr>  
				        <td style="width:90px;" align="right">备注: </td>
				        <td style="padding:10px;" class="input-icon input-icon-right">				        	
						 <input type="text" name="remark" id="updateRemark" autocomplete="off" maxlength="50" style="width: 230px;"/>  						   						  
				        </td>
				    </tr>							
				   										   
					<tr>
					    <td colspan='2'>
					    	<button class="btn btn-info btn-sm" type="submit" onclick="updateZhj()" style='border-radius:8px; margin-left:27%;'>
								<i class="icon-ok bigger-110"></i>确定
							</button>
						    <span class="btn btn-sm" onclick="$('#updateZhj').dialog('close')" style='border-radius:8px; margin-left:40px;'>
								<i class="icon-remove bigger-110"></i>取消
							</span>
						</td>
					</tr>  
				</table>					   					   				
	 		</form>								
		</div>				
						
						
						
		<div id="deleteZhjDialog" title="删除指挥机" style="display:none;"> 
    	   
    	    <table  style="font-size:13px; text-align:right;border-collapse:separate;border-spacing:0px 20px;">  										    				    				    
				    <tr>  
				        <td style="width:150px;" align="right">确认删除所选记录吗？</td>
				       
				    </tr>				    				  				   							   
					<tr >
					    <td colspan='2'>
					    	<button class="btn btn-info btn-sm" type="submit" onclick="deleteZhj()" style='border-radius:8px;'>
								<i class="icon-ok bigger-110"></i>确定
							</button>
						    
						</td>
						 <td colspan='2'>
					    	
						    <span class="btn btn-sm" onclick="$('#deleteZhjDialog').dialog('close')" style='border-radius:8px; margin-left:40px;'>
								<i class="icon-remove bigger-110"></i>取消
							</span>
						</td>
					</tr>  
				</table>					
		</div>							
