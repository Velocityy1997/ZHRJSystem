@charset "utf-8";
/* CSS Document */

div.loginArea{
	padding-bottom: 60px;
}
div.loginArea div.title{
	position: relative;
	padding: 0;
}
div.loginArea div.title h5{
	position: relative;
	padding: 0 20px;
	height: 31px;
	border-bottom: 2px solid #cc0000;
}
div.loginArea div.title p{
	float: left;
	color: #999;
	text-indent: 10px;
}

div.progressBar dl,
div.progressBar dt,
div.progressBar dd{
	width: 200px;
}
