/*  ======================  content  ====================== */
div.loginArea div.content{
	width: 760px;
	margin: 0 auto;
}
 .tabs{
	height: 31px;
	position: relative;
	width: 1000px;
	left: -1px;
}
 .tabs li{
	float: left;
	height: 30px;
	line-height: 30px;
	background: url(../img/keySearch_title_li.jpg) repeat-x;
	border-left: 1px solid #dcdddd;
	border-bottom: 1px solid #dcdddd;
	text-align: center;
	cursor: pointer;
	width: 165px;
}
 .tabs li a{
	display: block;
	color: #515151;
}
 .tabs li.on{
	background: #fff;
	border-bottom: 1px solid #fff;
	font-weight: bold;
	width: 168px;
}
div.keySearch .tabs li.on a{
	font-weight: bold;
}

div.loginArea div.content div.tit{
	border-bottom: 1px solid #cc0000;
	position: relative;
	height: 28px;
}
div.loginArea div.content div.tit h1{
	position: relative;
	color: #cc0000;
	width: 500px;
	
	float: left;
	height: 20px;
	margin-top:0px;
	margin-bottom:5px;
	font-size:20px;
	padding-bottom: 7px;
	top: 6px;
	background: url(img/icon_triangle_dw_red.png) no-repeat center bottom;
}
div.loginArea div.con{
	padding-top: 10px;
}
div.loginArea div.con li{
	clear: both;
	list-style-type:none;
	height: 40px;
}



div.loginArea div.con li span,
div.loginArea div.con li label,
div.loginArea div.con li input,
div.loginArea div.con li select,
div.loginArea div.con li img{
	float: left;
}
div.loginArea div.con li span.name{
	display: block;
	text-align: right;
	width: 130px;
	margin: 7px 5px 0 0;
}

li{
	padding: 20px;
}

div.loginArea div.con li span.name em{
	color: #c00;
	margin-right: 5px;
}
div.loginArea div.con li input[type=text],
div.loginArea div.con li input[type=password]{
	width: 250px;
	height: 28px;
	border: 1px solid #dcdddd;
	text-indent: 5px;
	line-height: 28px;
	
}
div.loginArea div.con li input.inputText{
	_width: 250px;
	_height: 31px;
	_border: 1px solid #dcdddd;
	_text-indent: 5px;
	
	_line-height: 31px;
}


