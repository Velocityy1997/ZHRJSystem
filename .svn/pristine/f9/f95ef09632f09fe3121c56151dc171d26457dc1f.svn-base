div.loginArea div.con li span.tip{
	margin: 7px 0 0 20px;
	
	float: left;
}
div.loginArea div.con li select{
	width: 90px;
	height: 30px;
	border: 1px solid #dcdddd;
	margin: 0px 7px 0 0;
}
div.loginArea div.btn{
	margin: 30px 0 0 350px;
}
.btnadpt{
	width:100px;
	height:45px;
	line-height:45px;
	display:block;
	
	margin-left:400px;
	font-size:15px;
	color:#fff;
	cursor:pointer}
	.btn_orange{
	
	background-color:#415F87
	}

div.companyInfo,div.basicInfo,div.ruleInfo,div.fujian{
	margin-top: 35px;
}

div.ruleInfo textarea{
	width: 680px;
	height: 200px;
	padding: 5px;
	border: 1px solid #dcdddd;
	line-height: 22px;
	margin: 0 0 0 35px;
}
div.ruleInfo label{
	margin: 15px 0 0 35px;
}
div.ruleInfo label input{
	margin-right: 5px;
}
